local co1 = coroutine.create(try1)
local co2 = coroutine.create(try2)
local co3 = coroutine.create(try3)
local co4 = coroutine.create(try4)
local co5 = coroutine.create(try5)

game:GetService("RunService").RenderStepped:connect(function()
	coroutine.resume(co1)
	--[[
	coroutine.resume(co2)
    coroutine.resume(co3)
	coroutine.resume(co4)
	coroutine.resume(co5)]]
	game:GetService("RunService").RenderStepped:wait()
end)


    

local is_on = function()
	return on and "on" or "off"
end

local mouse = plr:GetMouse()
mouse.KeyDown:connect(
    function(key)
	if key == "q" then
		on = not on
	elseif key == "t" then
		local status = is_on()
		starterGui:SetCore(
                "SendNotification",
                {
			Title = "RivalWare",
			Text = "Reach Status " .. status .. ". Current: " .. distance .. " Reach"
		}
            )
	elseif key == "j" then
		distance = distance + 1
		starterGui:SetCore(
                "SendNotification",
                {
			Title = "RivalWare",
			Text = "Reach has increased by 1. Current: " .. distance .. " CPS"
		}
            )
	elseif key == "k" then
		if distance > 1 then
			distance = distance - 1
			starterGui:SetCore(
                    "SendNotification",
                    {
				Title = "RivalWare",
				Text = "Reach has decreased by 1. Current: " .. distance .. " CPS"
			}
                )
		else
			starterGui:SetCore("SendNotification", {
				Title = "RivalWare",
				Text = "Cannot decrease any further!"
			})
		end
	end
end
)
    

