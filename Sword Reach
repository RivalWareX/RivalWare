on = true
distance = 6 -- You can edit this to however long you want your reach

starterGui = game:GetService("StarterGui")
plr = game:GetService("Players").LocalPlayer


function try1()
        game:GetService("RunService").RenderStepped:connect(
            function()
        	if on then
        		local hum = plr.Character.Humanoid
        		for _, v in ipairs(game.Players:GetPlayers()) do
        			if v.Name ~= plr.Name then
        				local s, e =
                                pcall(
                                function()
        					local root = v.Character["Right Leg"]
        					local root2 = v.Character["Left Leg"]
        					local root3 = v.Character["Right Arm"]
        					local root4 = v.Character["Left Arm"]
        					local root5 = v.Character.Head
        					local root6 = v.Character.Torso
        					local root7 = v.Character.HumanoidRootPart
        					local root8 = v.Character.PrimaryPart
        					local sword = hum.Parent:FindFirstChildOfClass("Tool")
        					if sword and (root.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= distance then
        					    if root6.Transparency ~= 1 then
                					        firetouchinterest(sword.Handle, root, 1)
        									firetouchinterest(sword.Handle, root, 0)
        									firetouchinterest(sword.Handle, root2, 1)
        									firetouchinterest(sword.Handle, root2, 0)
        									firetouchinterest(sword.Handle, root3, 1)
        									firetouchinterest(sword.Handle, root3, 0)
        									firetouchinterest(sword.Handle, root4, 1)
        									firetouchinterest(sword.Handle, root4, 0)
        									firetouchinterest(sword.Handle, root5, 1)
        									firetouchinterest(sword.Handle, root5, 0)
        									firetouchinterest(sword.Handle, root6, 1)
        									firetouchinterest(sword.Handle, root6, 0)
        									firetouchinterest(sword.Handle, root7, 1)
        									firetouchinterest(sword.Handle, root7, 0)
        									firetouchinterest(sword.Handle, root8, 1)
        									firetouchinterest(sword.Handle, root8, 0)
        									print("c")
        					    end
        				    end
        				end
                            )
        			end
        		end
        	end
        end
        )
        coroutine.yield()
end

